<+>.title-section {\r\n    margin-left: 10%;\r\n    width: 80%;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    padding-top: 0;\r\n}\r\n\r\n.title-content {\r\n    flex: 1;\r\n}\r\n\r\n.title-section h1 {\r\n    font-family: 'Luckiest Guy', cursive;\r\n    font-size: 90px;\r\n    font-weight: normal;\r\n    line-height: 1.5;\r\n    text-align: left;\r\n    margin: 0;\r\n}\r\n.title-section a{\r\n    font-family: Poppins, Helvetica, sans-serif;\r\n    margin-top: 50px;\r\n    margin-left: 20px;\r\n}\r\n\r\n.title-section p {\r\n    font-family: Poppins, Helvetica, sans-serif;\r\n    font-size: 16px;\r\n    font-family: sans-serif;\r\n    color: #555;\r\n    margin-top: 10px;\r\n    margin-bottom: 30px;\r\n}\r\n\r\n.title-section .btn {\r\n    text-decoration: none;\r\n    color: white;\r\n    font-size: 20px;\r\n    padding: 12px 20px;\r\n    background-color: #9A60E9;\r\n    border-radius: 10px;\r\n    transition: background 0.3s;\r\n    display: inline-block;\r\n    font-weight: bold;\r\n}\r\n\r\n.title-section .btn:hover {\r\n    background-color: black;\r\n}\r\n\r\n.title-image {\r\n    max-width: 500px;\r\n    margin-left: 5%;\r\n}\r\n\r\n.tile-container {\r\n    width: 100%;\r\n    margin-left: 250px;\r\n    align-items: center;\r\n    margin-top: 100px;\r\n    display: grid;\r\n    grid-template-columns: repeat(auto-fill, minmax(150px, 1fr));\r\n    gap: 300px;\r\n    margin-bottom: 60px;\r\n}\r\n\r\n.tile {\r\n    font-family: Poppins, Helvetica, sans-serif;\r\n    background-color: lightgray;\r\n    width: 300px;\r\n    height: 300px;\r\n    border-radius: 20px;\r\n    padding-top: 90px;\r\n    padding-left: 20px;\r\n    padding-right: 20px;\r\n    padding-bottom: 90px;\r\n    text-align: center;\r\n}\r\n#logo-img {\r\n    width: 130px;\r\n    height: auto; /* Maintains aspect ratio */\r\n}\r\n.section-two {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    padding: 40px;\r\n    position: relative;\r\n}\r\n\r\n.chat-left, .chat-right {\r\n    display: flex;\r\n    align-items: center;\r\n    position: relative;\r\n}\r\n\r\n.chat-left {\r\n    flex-direction: row;\r\n}\r\n\r\n.chat-right {\r\n    flex-direction: row-reverse;\r\n}\r\n\r\n.avatar {\r\n    width: 60px;\r\n    height: 60px;\r\n}\r\n/*----------------------------------*/\r\n.card-container {\r\n    font-family: Poppins, Helvetica, sans-serif;\r\n    width: 100%;\r\n    margin-top: 60px;\r\n    margin-left: 500px;\r\n    background: white;\r\n    border-radius: 25px;\r\n    padding: 30px;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    max-width: 900px;\r\n    box-shadow: 0 8px 24px rgba(154, 112, 245, 0.15);\r\n    position: relative;\r\n    width: 90%;\r\n    transition: transform 0.3s ease, box-shadow 0.3s ease;\r\n}\r\n\r\n.card-container:hover {\r\n    transform: scale(1.02);\r\n    box-shadow: 0 12px 32px rgba(154, 112, 245, 0.25);\r\n}\r\n\r\n.arrow {\r\n    font-size: 36px;\r\n    font-weight: bold;\r\n    color: #9a60e9;\r\n    cursor: pointer;\r\n    transition: color 0.3s ease;\r\n}\r\n\r\n.arrow:hover {\r\n    color: black;\r\n}\r\n\r\n.arrow.left {\r\n    margin-right: 100px;\r\n    margin-left: 0;\r\n}\r\n\r\n.arrow.right {\r\n    margin-left: 100px;\r\n    margin-right: 0;\r\n}\r\n\r\n.profile-img {\r\n    width: 120px;\r\n    height: 120px;\r\n    border-radius: 50%;\r\n    object-fit: cover;\r\n    border: 4px solid #9a60e9;\r\n    transition: transform 0.3s;\r\n}\r\n\r\n.profile-img:hover {\r\n    transform: scale(1.05);\r\n}\r\n\r\n.testimonial-content {\r\n    width: 300px;\r\n    margin-left: 100px;\r\n    font-size: 18px;\r\n    color: #444;\r\n    text-align: left;\r\n}\r\n\r\n.testimonial-content p {\r\n    font-family: Poppins, Helvetica, sans-serif;\r\n    margin: 0;\r\n    font-style: italic;\r\n    line-height: 1.6;\r\n}\r\n\r\n.testimonial-content .author {\r\n    margin-top: 15px;\r\n    font-weight: bold;\r\n    color: #9a60e9;\r\n}\r\n\r\n.dots {\r\n    display: flex;\r\n    justify-content: center;\r\n    margin-top: 20px;\r\n}\r\n\r\n.dot {\r\n    width: 10px;\r\n    height: 10px;\r\n    border-radius: 50%;\r\n    background-color: #d2bdf4;\r\n    margin: 0 6px;\r\n    transition: background-color 0.3s ease;\r\n}\r\n\r\n.dot.active {\r\n    background-color: #9a60e9;\r\n}\r\n
